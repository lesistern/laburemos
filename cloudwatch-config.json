{
    "agent": {
        "metrics_collection_interval": 60,
        "run_as_user": "cwagent"
    },
    "logs": {
        "logs_collected": {
            "files": {
                "collect_list": [
                    {
                        "file_path": "/home/ubuntu/laburemos/backend/logs/combined.log",
                        "log_group_name": "laburemos-backend",
                        "log_stream_name": "application-logs",
                        "timezone": "UTC"
                    },
                    {
                        "file_path": "/home/ubuntu/laburemos/backend/logs/err.log",
                        "log_group_name": "laburemos-backend",
                        "log_stream_name": "error-logs",
                        "timezone": "UTC"
                    },
                    {
                        "file_path": "/var/log/nginx/access.log",
                        "log_group_name": "laburemos-nginx",
                        "log_stream_name": "access-logs",
                        "timezone": "UTC"
                    },
                    {
                        "file_path": "/var/log/nginx/error.log",
                        "log_group_name": "laburemos-nginx",
                        "log_stream_name": "error-logs",
                        "timezone": "UTC"
                    }
                ]
            }
        }
    },
    "metrics": {
        "namespace": "LABUREMOS/Production",
        "metrics_collected": {
            "cpu": {
                "measurement": [
                    {
                        "name": "cpu_usage_idle",
                        "rename": "CPU_USAGE_IDLE",
                        "unit": "Percent"
                    },
                    {
                        "name": "cpu_usage_iowait",
                        "rename": "CPU_USAGE_IOWAIT",
                        "unit": "Percent"
                    }
                ],
                "metrics_collection_interval": 60,
                "totalcpu": false
            },
            "disk": {
                "measurement": [
                    {
                        "name": "used_percent",
                        "rename": "DISK_USED_PERCENT",
                        "unit": "Percent"
                    }
                ],
                "metrics_collection_interval": 60,
                "resources": [
                    "*"
                ]
            },
            "diskio": {
                "measurement": [
                    {
                        "name": "io_time"
                    }
                ],
                "metrics_collection_interval": 60,
                "resources": [
                    "*"
                ]
            },
            "mem": {
                "measurement": [
                    {
                        "name": "mem_used_percent",
                        "rename": "MEMORY_USED_PERCENT",
                        "unit": "Percent"
                    }
                ],
                "metrics_collection_interval": 60
            },
            "netstat": {
                "measurement": [
                    {
                        "name": "tcp_established",
                        "rename": "TCP_ESTABLISHED"
                    },
                    {
                        "name": "tcp_time_wait",
                        "rename": "TCP_TIMEWAIT"
                    }
                ],
                "metrics_collection_interval": 60
            },
            "swap": {
                "measurement": [
                    {
                        "name": "swap_used_percent",
                        "rename": "SWAP_USED_PERCENT",
                        "unit": "Percent"
                    }
                ],
                "metrics_collection_interval": 60
            }
        }
    }
}