{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "tsconfig.json"
  },
  "projects": {
    "auth": {
      "type": "application",
      "root": "src/auth",
      "entryFile": "main",
      "sourceRoot": "src/auth",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.json"
      }
    },
    "user": {
      "type": "application", 
      "root": "src/user",
      "entryFile": "main",
      "sourceRoot": "src/user",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.json"
      }
    },
    "project": {
      "type": "application",
      "root": "src/project", 
      "entryFile": "main",
      "sourceRoot": "src/project",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.json"
      }
    },
    "payment": {
      "type": "application",
      "root": "src/payment",
      "entryFile": "main", 
      "sourceRoot": "src/payment",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.json"
      }
    },
    "notification": {
      "type": "application",
      "root": "src/notification",
      "entryFile": "main",
      "sourceRoot": "src/notification", 
      "compilerOptions": {
        "tsConfigPath": "tsconfig.json"
      }
    }
  }
}